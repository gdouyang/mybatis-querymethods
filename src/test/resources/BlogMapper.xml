<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="querymethods.BlogMapper">
  <select id="selectBlog" resultType="querymethods.Blog">
    select * from blog where id = #{id}
  </select>
  
  <insert id="insert1" parameterType="querymethods.Blog">
  	insert into blog(id, firstName, lastName) values(#{id}, #{firstName}, #{lastName})
  </insert>
</mapper>